{
  "name": "@xrr2016/json-editor",
  "version": "1.1.8",
  "description": "",
  "author": "https://github.com/xrr2016",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xrr2016/json-editor.git"
  },
  "keywords": [
    "vue",
    "component"
  ],
  "files": [
    "src",
    "dist"
  ],
  "main": "dist/json-editor.umd.js",
  "module": "dist/json-editor.esm.js",
  "unpkg": "dist/json-editor.min.js",
  "browser": {
    "./sfc": "src/json-editor.vue"
  },
  "scripts": {
    "dev": "vue-styleguidist server",
    "test": "jest --verbose",
    "doc": "vue-styleguidist build",
    "build": "npm run build:unpkg & npm run build:es & npm run build:umd & npm run doc",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/json-editor.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/json-editor.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/json-editor.min.js",
    "stdver": "standard-version -m '[skip ci] chore(release): v%s'",
    "release": "gren release --override",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@vue/babel-preset-app": "^3.11.0",
    "babel-loader": "^8.0.5",
    "element-ui": "^2.12.0",
    "file-loader": "^3.0.1",
    "github-release-notes": "^0.17.0",
    "glob": "^7.1.3",
    "husky": "1.3.1",
    "jest": "^24.8.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.0",
    "minimist": "^1.2.0",
    "prettier": "1.18.2",
    "rollup": "^1.9.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "standard-version": "^8.0.1",
    "stylelint": "^9.10.0",
    "stylelint-config-standard": "^18.2.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-styleguidist": "^3.16.3",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.29.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "vue-sfc-cli": "1.10.8",
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.(js|md|json)": [
      "prettier --write",
      "git add"
    ],
    "*.vue": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  }
}
